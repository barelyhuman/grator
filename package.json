{
	"name": "@barelyhuman/grator",
	"version": "1.0.3",
	"description": "A mini tool to help with migration sql files",
	"license": "MIT",
	"repository": "barelyhuman/grator",
	"private": false,
	"source": "cli.mjs",
	"author": {
		"name": "Reaper",
		"email": "ahoy@barelyhuman.dev",
		"url": "https://reaper.im"
	},
	"type": "module",
	"bin": {
		"grator": "dist/cli.cjs"
	},
	"engines": {
		"node": ">=12"
	},
	"scripts": {
		"test": "xo && ava",
		"build": "microbundle --no-sourcemap --target node -f cjs --o dist/cli.cjs; mv dist/cli.cjs.js dist/cli.cjs ",
		"fix": "standard --fix ."
	},
	"files": [
		"dist/**/*.js"
	],
	"keywords": [
		"cli",
		"grator",
		"migrations",
		"sql",
		"postgres"
	],
	"dependencies": {
		"@barelyreaper/conch": "0.0.4",
		"knex": "^0.95.4",
		"meow": "^9.0.0",
		"ora": "^5.4.0",
		"pg": "^8.6.0"
	},
	"devDependencies": {
		"ava": "^3.15.0",
		"execa": "^5.0.0",
		"microbundle": "^0.13.0",
		"standard": "^16.0.3",
		"xo": "^0.37.0"
	},
	"standard": {
		"ignore": [
			"node_modules"
		]
	}
}
